{
    "buildCommand": "npx expo export -p web",
    "outputDirectory": "dist",
    "framework": null,
    "installCommand": "npm install",
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node@3.0.0"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        }
    ]
}